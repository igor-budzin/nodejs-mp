{
  "name": "nodejs-mp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "task1": "node ./Task_3.1/test.js",
    "task2": "node ./Task_3.2/test.js",
    "test3": "node ./Task_3.3/index.js",
    "start": "nodemon --watch app/** --ext ts,json --exec ts-node --files app/index.ts",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=app/db/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=app/db/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=app/db/data-source.ts migration:revert",
    "seed:run": "ts-node -r tsconfig-paths/register ./app/db/seeds/run-seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igor-budzin/nodejs-mp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igor-budzin/nodejs-mp/issues"
  },
  "homepage": "https://github.com/igor-budzin/nodejs-mp#readme",
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.6",
    "@types/uuid": "^9.0.5",
    "env-cmd": "^10.1.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
